	function getRenderedSize(cWidth, cHeight, width, height){
	  var oRatio = width / height,
	      cRatio = cWidth / cHeight;
	  return function() {
	    if (oRatio > cRatio) {
	      this.width = cWidth;
	      this.height = cWidth / oRatio;
	    } else {
	      this.width = cHeight * oRatio;
	      this.height = cHeight;
	    }      
	    return this;
	  }.call({});
	}
	function getImgSizeInfo(obj) {
		var img=obj.get(0);
		return getRenderedSize(
	                         img.width,
	                         img.height,
	                         img.naturalWidth,
	                         img.naturalHeight);
	}


