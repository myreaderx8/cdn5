	        $('.arrnav .backward').click();
	    }
	    if (key == 39){ //right arrow key
			stopShow();
	        $('.arrnav .forward').click();
	    }
	    if (key == 32){ //space key
			if($('.arrnav .nav.play').is('.pause')){
				$('div.tabs').data('slideshow').play()
